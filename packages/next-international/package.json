{
  "name": "next-international",
  "version": "0.6.0",
  "description": "Type-safe internationalization (i18n) for Next.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./client": {
      "types": "./dist/app/client/index.d.ts",
      "default": "./dist/app/client/index.js"
    },
    "./server": {
      "types": "./dist/app/server/index.d.ts",
      "default": "./dist/app/server/index.js"
    },
    "./middleware": {
      "types": "./dist/app/middleware/index.d.ts",
      "default": "./dist/app/middleware/index.js"
    }
  },
  "keywords": [
    "next",
    "i18n",
    "translate",
    "internationalization"
  ],
  "files": [
    "dist",
    "client.d.ts",
    "server.d.ts",
    "middleware.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuiiBz/next-international.git"
  },
  "bugs": {
    "url": "https://github.com/QuiiBz/next-international/issues"
  },
  "homepage": "https://github.com/QuiiBz/next-international#readme",
  "scripts": {
    "build": "tsup src/index.ts src/app/client/index.ts src/app/server/index.ts src/app/middleware/index.ts --dts --minify",
    "watch": "tsup src/index.ts src/app/client/index.ts src/app/server/index.ts src/app/middleware/index.ts --dts --watch"
  },
  "devDependencies": {
    "@types/react": "^18.2.9",
    "next": "13.4.9",
    "react": "18.2.0",
    "tsup": "^6.1.3"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^12.0.0 || ^11.0.0",
    "react": "^18.0.0 || ^17.0.0"
  },
  "dependencies": {
    "client-only": "^0.0.1",
    "international-types": "^0.6.0",
    "server-only": "^0.0.1"
  }
}
